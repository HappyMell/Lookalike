<body>
<div class="container-fluid">
<div class="row">

<% header = typeof header !== 'object' ? {} : header %>

  <aside id="nav" class="navbar-expand-lg navbar-light bg-light  align-items-start">
     <a class="navbar-brand" href="/"> <img class="bandlogo" src="/images/bandlogo.png"> </a>
      <button class="navbar-toggler" type="button" data-toggle="collapse" data-target="#navbarCollapse" aria-controls="navbarCollapse" aria-expanded="false" aria-label="Toggle navigation">
            <span class="navbar-toggler-icon"></span>
      </button>
    <nav class=" collapse navbar-collapse" id="navbarCollapse">
    <ul class="navbar-nav flex-lg-column  justify-content-between">
      <li class="nav-item <%= typeof page !== 'undefined' && page === 'home' ? 'active' : '' %>">
       <a class="nav-link" href="/">NEWS</a>
       </li>
       <li class="nav-item <%= typeof page !== 'undefined' && page === 'discography' ? 'active' : '' %>">
        <a class="nav-link" href="/discography">DISCOGRAPHY</a>
        </li>
        <li class="nav-item <%= typeof page !== 'undefined' && page === 'live' ? 'active' : '' %>">
        <a class="nav-link" href="#">LIVE</a>
        </li>      
        <li class="nav-item  <%= typeof page !== 'undefined' && page === 'store' ? 'active' : '' %>" >
        <a class="nav-link store" href="/store">STORE</a>
        <%if (header.url == "/store" || header.url =="/music" || header.url=="/clothing" || header.url == "/objects") {%>
                <img class="crossStore" src="images/cross.png" data-toggle="collapse" role="button" aria-expended="false" aria-controls="#storeCollapse" href="#storeCollapse" >
                   </li>
                   <div class="collapse show" id="storeCollapse">
                      <ul>
                        <li><a class="nav-link" href="/music">Music</a></li>
                        <li><a class="nav-link" href="/clothing">Clothing</a></li>
                        <li><a class="nav-link" href="/objects">Objects</a></li>
                      </ul>
                   </div>
                    <%}%>
                  
        <li class="nav-item <%= typeof page !== 'undefined' && page === 'follow' ? 'active' : '' %>">
        <a class="nav-link" href="/social-media">FOLLOW</a>
        </li>
        </ul>
</nav>
</aside>









